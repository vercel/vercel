{
  "version": 2,
  "builds": [
    {
      "src": "/api/go-one/one.go",
      "use": "@vercel/go",
      "config": { "parallel": true }
    },
    {
      "src": "/api/go-two/two.go",
      "use": "@vercel/go",
      "config": { "parallel": true }
    }
  ],
  "probes": [
    {
      "path": "/api/go-one/one.go",
      "mustContain": "ONE:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/api/go-two/two.go",
      "mustContain": "TWO:RANDOMNESS_PLACEHOLDER"
    }
  ]
}
