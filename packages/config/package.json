{
  "name": "@vercel/config",
  "version": "0.0.23",
  "description": "A TypeScript SDK for programmatically configuring Vercel projects",
  "bugs": {
    "url": "https://github.com/vercel/vercel/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/config"
  },
  "author": "Vercel",
  "license": "MIT",
  "main": "dist/v1/index.js",
  "types": "dist/v1/index.d.ts",
  "typesVersions": {
    "*": {
      "v1": [
        "dist/v1/index.d.ts"
      ],
      "v1/types": [
        "dist/v1/types.d.ts"
      ]
    }
  },
  "exports": {
    "./v1": "./dist/v1/index.js",
    "./v1/types": {
      "types": "./dist/v1/types.d.ts"
    }
  },
  "bin": {
    "@vercel/config": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "generate-types": "tsx scripts/generate-types.ts",
    "test": "vitest"
  },
  "dependencies": {
    "pretty-cache-header": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "tsx": "4.7.0",
    "typescript": "4.9.5",
    "vitest": "2.0.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
