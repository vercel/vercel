{
  "name": "@vercel/config",
  "version": "0.0.24",
  "description": "A TypeScript SDK for programmatically configuring Vercel projects",
  "license": "MIT",
  "homepage": "https://vercel.com/docs/projects/project-configuration",
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/config"
  },
  "main": "dist/v1/index.js",
  "types": "dist/v1/index.d.ts",
  "typesVersions": {
    "*": {
      "v1": [
        "dist/v1/index.d.ts"
      ],
      "v1/types": [
        "dist/v1/types.d.ts"
      ]
    }
  },
  "exports": {
    "./v1": "./dist/v1/index.js",
    "./v1/types": {
      "types": "./dist/v1/types.d.ts"
    }
  },
  "bin": {
    "@vercel/config": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "generate-types": "tsx scripts/generate-types.ts",
    "vitest-run": "vitest -c ../../vitest.config.mts",
    "vitest-unit": "glob --absolute 'src/**/*.test.ts'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "pretty-cache-header": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "14.18.33",
    "tsx": "4.7.0",
    "typescript": "4.9.5",
    "vite": "^5.1.6",
    "vitest": "^2.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
