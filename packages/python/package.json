    "@vercel/build-utils": "workspace:*",
    "@vercel/error-utils": "workspace:*",
    "cross-env": "7.0.3",
    "execa": "^1.0.0",
    "fs-extra": "11.1.1",
    "jest-junit": "16.0.0",
    "minimatch": "10.1.1",
    "pip-requirements-js": "1.0.2",
