{
  "name": "@vercel/introspection",
  "version": "0.0.10",
  "license": "Apache-2.0",
  "homepage": "https://vercel.com/docs",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./loaders/cjs": "./dist/loaders/cjs.js",
    "./loaders/esm": "./dist/loaders/esm.js",
    "./loaders/hooks": "./dist/loaders/hooks.js",
    "./loaders/rolldown-esm": "./dist/loaders/rolldown-esm.js",
    "./loaders/rolldown-hooks": "./dist/loaders/rolldown-hooks.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/introspection"
  },
  "scripts": {
    "build": "tsdown",
    "vitest-run": "vitest -c ../../vitest.config.mts",
    "vitest-unit": "glob --absolute 'test/unit/**/*.test.ts' 'test/unit/**/*.test.mts'",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vercel/backends": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "20.11.0",
    "tsdown": "0.16.3",
    "vitest": "^2.0.1"
  }
}
