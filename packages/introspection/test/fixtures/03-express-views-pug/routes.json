{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^(?:\\/user\\/([^\\/]+))(?:\\/$)?$",
      "dest": "/user/:id",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "^(?:\\/api\\/data)(?:\\/$)?$",
      "dest": "/api/data",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "framework": {
    "slug": "",
    "version": ""
  },
  "additionalFolders": [
    "pug-views"
  ],
  "additionalDeps": [
    "pug"
  ]
}