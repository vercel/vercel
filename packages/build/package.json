{
  "name": "@vercel/build",
  "version": "0.0.0",
  "description": "Build orchestration utilities for Vercel CLI",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/build"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "node ../../utils/build.mjs",
    "vitest-run": "vitest -c ../../vitest.config.mts",
    "vitest-unit": "glob --absolute 'test/unit.*.test.ts'",
    "type-check": "tsc --noEmit"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@vercel/build-utils": "workspace:*",
    "@vercel/frameworks": "workspace:*",
    "@vercel/routing-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/minimatch": "^5.1.2",
    "@types/node": "14.18.33",
    "@types/semver": "6.0.0",
    "fs-extra": "10.0.0",
    "minimatch": "3.1.2",
    "semver": "6.3.1",
    "typescript": "4.9.5",
    "vitest": "2.1.4"
  }
}
