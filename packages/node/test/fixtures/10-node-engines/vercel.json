{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "probes": [
    { "path": "/empty", "mustContain": "RANDOMNESS_PLACEHOLDER:18" },
    { "path": "/greater", "mustContain": "RANDOMNESS_PLACEHOLDER:18" },
    { "path": "/major", "mustContain": "RANDOMNESS_PLACEHOLDER:16" },
    { "path": "/range", "mustContain": "RANDOMNESS_PLACEHOLDER:16" }
  ]
}
