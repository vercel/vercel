{
  "builds": [{ "src": "api/**/*.{js,mjs}", "use": "@vercel/node" }],
  "probes": [
    {
      "path": "/api/edge.js",
      "mustContain": "RANDOMNESS_PLACEHOLDER:edge"
    },
    {
      "path": "/api/wasm.js",
      "mustContain": "RANDOMNESS_PLACEHOLDER:edge, 11."
    },
    {
      "path": "/api/react-dom.mjs",
      "mustContain": "<h1>RANDOMNESS_PLACEHOLDER:Hello from Edge</h1>"
    }
  ]
}
