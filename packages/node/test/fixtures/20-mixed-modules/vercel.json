{
  "version": 2,
  "builds": [{ "src": "entrypoint**", "use": "@vercel/node" }],
  "probes": [
    {
      "path": "/entrypoint.js",
      "mustContain": "mixed-modules:js:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/entrypoint.mjs",
      "mustContain": "mixed-modules:mjs:RANDOMNESS_PLACEHOLDER"
    },
    {
      "path": "/entrypoint.ts",
      "mustContain": "mixed-modules:ts:RANDOMNESS_PLACEHOLDER"
    }
  ]
}
