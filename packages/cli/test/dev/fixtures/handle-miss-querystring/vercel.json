{
  "version": 2,
  "builds": [
    { "src": "functions/echo.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "^/echo/([^/]+)/([^/]+)$",
      "dest": "/functions/echo.js?a=$1&b=$2"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/functions(/.*)?$",
      "status": 404,
      "continue": true
    },
    {
      "handle": "miss"
    },
    {
      "src": "^/functions/(.+)(?:\\.(?:js))$",
      "dest": "/functions/$1",
      "check": true
    }
  ]
}
