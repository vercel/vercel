{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/next" }],
  "probes": [
    {
      "path": "/_next/__NEXT_SCRIPT__(/hello)",
      "mustContain": "hello world"
    },
    {
      "path": "/something",
      "mustContain": "something"
    }
  ],
  "env": {
    "NEXT_BUILDER_INTEGRATION": "1"
  }
}
