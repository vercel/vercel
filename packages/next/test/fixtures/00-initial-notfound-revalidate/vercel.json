{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "probes": [
    {
      "path": "/",
      "status": 200,
      "mustContain": "index page",
      "retries": 10,
      "retryDelay": 3000
    },
    {
      "path": "/another",
      "status": 200,
      "mustContain": "another page",
      "retries": 10,
      "retryDelay": 3000
    },
    {
      "path": "/blog/first",
      "status": 200,
      "mustContain": "dynamic page",
      "retries": 10,
      "retryDelay": 3000
    }
  ]
}
