{
  "version": 2,
  "uploadNowJson": true,
  "builds": [
    {
      "src": "apps/app/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/app/$1"
    }
  ],
  "probes": [
    {
      "path": "/",
      "status": 200,
      "mustContain": "index page"
    },
    {
      "path": "/another",
      "status": 200,
      "mustContain": "another page"
    }
  ]
}
