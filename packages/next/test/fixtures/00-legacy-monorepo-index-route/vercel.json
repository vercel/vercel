{
  "uploadNowJson": true,
  "builds": [
    {
      "src": "www/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/widgets/(?<id>.*)",
      "dest": "/www/index?id=$id"
    }
  ],
  "probes": [
    {
      "path": "/www/index",
      "status": 200,
      "mustContain": "index page"
    },
    {
      "path": "/widgets/5",
      "status": 200,
      "mustContain": "index page"
    },
    {
      "path": "/www/first",
      "status": 200,
      "mustContain": "id page"
    },
    {
      "path": "/www/blog",
      "status": 200,
      "mustContain": "blog index page"
    },
    {
      "path": "/www/blog/first",
      "status": 200,
      "mustContain": "blog slug page"
    }
  ]
}
