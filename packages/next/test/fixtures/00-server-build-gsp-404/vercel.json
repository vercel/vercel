{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "probes": [
    {
      "path": "/",
      "status": 200,
      "mustContain": "index page"
    },
    {
      "path": "/another",
      "status": 200,
      "mustContain": "another page"
    },
    {
      "path": "/static",
      "status": 200,
      "mustContain": "static page"
    },
    {
      "path": "/blog/missing",
      "status": 404,
      "mustContain": "custom 404"
    }
  ]
}
