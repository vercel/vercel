{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "probes": [
    {
      "path": "/docs/test.png",
      "status": 200
    },
    {
      "path": "/docs",
      "status": 200,
      "mustContain": "Index page"
    },
    {
      "path": "/docs/_next/image?url=%2Fdocs%2Ftest.png&w=48&q=75",
      "status": 200,
      "responseHeaders": {
        "content-type": "image/png"
      }
    }
  ]
}
