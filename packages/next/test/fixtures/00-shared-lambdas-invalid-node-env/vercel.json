{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "probes": [
    {
      "path": "/",
      "status": 200,
      "mustContain": "index page"
    },
    {
      "path": "/another",
      "status": 200,
      "mustContain": "another page"
    },
    {
      "path": "/api/hello",
      "status": 200,
      "mustContain": "hello\":\"world\""
    },
    {
      "path": "/api/hello",
      "status": 200,
      "mustContain": "env\":\"production\""
    }
  ]
}
