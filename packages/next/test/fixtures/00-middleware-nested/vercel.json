{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/next" }],
  "probes": [
    {
      "path": "/redirect-me",
      "status": 307,
      "responseHeaders": {
        "Location": "/from-middleware/"
      },
      "fetchOptions": {
        "redirect": "manual"
      }
    }
  ]
}
