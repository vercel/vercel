{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@vercel/next" }],
  "probes": [
    {
      "path": "/",
      "status": 200,
      "mustContain": "index page"
    },
    {
      "delay": 3000
    },
    {
      "path": "/",
      "status": 200,
      "mustContain": "index page"
    },
    {
      "path": "/_next/data/testing-build-id/index.json",
      "status": 200,
      "mustContain": "page\":\"index\""
    },
    {
      "delay": 3000
    },
    {
      "path": "/_next/data/testing-build-id/index.json",
      "status": 200,
      "mustContain": "page\":\"index\""
    },
    {
      "delay": 3000
    },
    {
      "path": "/",
      "status": 200,
      "mustContain": "index page"
    }
  ]
}
