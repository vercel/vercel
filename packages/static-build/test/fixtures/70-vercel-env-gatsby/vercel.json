{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "zeroConfig": true, "framework": "gatsby" }
    }
  ],
  "probes": [
    { "path": "/index.json", "mustContain": "GATSBY_VERCEL_URL" },
    {
      "path": "/index.json",
      "mustContain": "TURBO_CI_VENDOR_ENV_KEY"
    }
  ]
}
