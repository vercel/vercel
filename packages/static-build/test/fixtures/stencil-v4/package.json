    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^4.19.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.68",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "21.1.1"
  },
