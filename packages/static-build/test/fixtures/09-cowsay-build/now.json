{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "zeroConfig": true }
    },
    {
      "src": "subdirectory/package.json",
      "use": "@vercel/static-build",
      "config": { "zeroConfig": true }
    }
  ],
  "probes": [
    { "path": "/", "mustContain": "cow:RANDOMNESS_PLACEHOLDER" },
    { "path": "/subdirectory/", "mustContain": "yoda:RANDOMNESS_PLACEHOLDER" }
  ]
}
