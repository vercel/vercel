{
  "name": "@vercel/python-analysis",
  "version": "0.3.0",
  "main": "./dist/index.js",
  "imports": {
    "#wasm/*": "./dist/wasm/*"
  },
  "license": "Apache-2.0",
  "homepage": "https://vercel.com/docs/runtimes#official-runtimes/python",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vercel/vercel.git",
    "directory": "packages/python-analysis"
  },
  "scripts": {
    "build": "pnpm generate:schemas && pnpm build:wasm && node ../../utils/build.mjs",
    "build:wasm": "node scripts/build-wasm.mjs",
    "generate:schemas": "ts-to-zod --all",
    "test": "vitest run --config ../../vitest.config.mts",
    "vitest-run": "vitest -c ../../vitest.config.mts",
    "vitest-unit": "glob --absolute 'test/*.test.ts'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@bytecodealliance/preview2-shim": "0.17.6",
    "@renovatebot/pep440": "4.2.1",
    "fs-extra": "11.1.1",
    "js-yaml": "4.1.1",
    "minimatch": "10.1.1",
    "pip-requirements-js": "1.0.2",
    "smol-toml": "1.5.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@bytecodealliance/jco-transpile": "0.1.1",
    "@types/fs-extra": "11.0.4",
    "@types/js-yaml": "4.0.9",
    "@types/node": "20.11.0",
    "ts-to-zod": "^3",
    "vitest": "2.1.4"
  }
}
