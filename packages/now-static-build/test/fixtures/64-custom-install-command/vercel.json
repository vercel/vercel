{
  "builds": [
    {
      "src": "build.js",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true,
        "installCommand": "node install.js",
        "buildCommand": "node build.js"
      }
    }
  ],
  "probes": [{ "path": "/", "mustContain": "installCommand\nbuildCommand" }]
}
