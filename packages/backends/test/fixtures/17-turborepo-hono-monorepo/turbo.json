{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "env": ["VERCEL"],
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", ".vercel/output/**", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
