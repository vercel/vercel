[
  {
    "handle": "filesystem"
  },
  {
    "src": "^(?:\\/echo)(?:\\/$)?$",
    "dest": "/echo",
    "methods": [
      "GET"
    ]
  },
  {
    "src": "^(?:\\/user\\/([^\\/]+))(?:\\/$)?$",
    "dest": "/user/:id",
    "methods": [
      "GET"
    ]
  },
  {
    "src": "^(?:\\/api\\/data)(?:\\/$)?$",
    "dest": "/api/data",
    "methods": [
      "GET"
    ]
  },
  {
    "src": "/(.*)",
    "dest": "/"
  }
]