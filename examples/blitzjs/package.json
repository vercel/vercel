    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@vitejs/plugin-react": "2.2.0",
    "eslint": "8.27.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "jsdom": "20.0.3",
